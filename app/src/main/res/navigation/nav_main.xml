        <action
            android:id="@+id/action_myWallFragment_to_eventFragment"
            app:destination="@id/eventFragment" />
        <action
            android:id="@+id/action_myWallFragment_to_createFragment"
            app:destination="@id/createFragment" />
        <action
            android:id="@+id/action_myWallFragment_to_postFragment"
            app:destination="@id/postFragment" />
        <action
            android:id="@+id/action_myWallFragment_to_wallFragment"
            app:destination="@id/wallFragment" />
